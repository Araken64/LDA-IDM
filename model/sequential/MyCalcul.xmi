<?xml version="1.0" encoding="UTF-8"?>
<LDP:Processus
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:LDP="http://www.univ-pau.fr/m2ti/2020/LDP"
    xsi:schemaLocation="http://www.univ-pau.fr/m2ti/2020/LDP ../metamodels/LDP.ecore">
  <activites
      description="multiplication"
      suivante="//@activites.1">
    <action
        methodName="multiplication"
        returnTag="resMult">
      <paramsTag>x1</paramsTag>
      <paramsTag>x2</paramsTag>
    </action>
  </activites>
  <activites
      description="racine carree"
      suivante="//@activites.2"
      precedente="//@activites.0">
    <action
        methodName="sqrt"
        returnTag="resSqrt">
      <paramsTag>x3</paramsTag>
    </action>
  </activites>
  <activites
      description="division"
      suivante="//@activites.3"
      precedente="//@activites.1">
    <action
        methodName="division"
        returnTag="resDiv">
      <paramsTag>resSqrt</paramsTag>
      <paramsTag>x4</paramsTag>
    </action>
  </activites>
  <activites
      description="plus"
      precedente="//@activites.2">
    <action
        methodName="addition"
        returnTag="res">
      <paramsTag>resMult</paramsTag>
      <paramsTag>resDiv</paramsTag>
    </action>
  </activites>
  <debut/>
  <fin/>
</LDP:Processus>
